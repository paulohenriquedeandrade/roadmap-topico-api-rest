{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.ts$",
    "excludePattern": "(node_modules/|dist/)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  }
}
